 
<?php $__env->startSection('content'); ?>
    <h2>Crear cita</h2>
 
    <?php echo Form::model(new App\Cita, ['route' => ['citas.store']]); ?>

        <?php echo $__env->make('citas/partials/_form', ['submit_text' => 'Create Cita'], array_except(get_defined_vars(), array('__data', '__path')))->render(); ?>
    <?php echo Form::close(); ?>

<?php $__env->stopSection(); ?>


<?php echo $__env->make('app', array_except(get_defined_vars(), array('__data', '__path')))->render(); ?>